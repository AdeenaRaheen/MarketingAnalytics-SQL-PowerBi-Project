TRUNCATE TABLE bronze.Customers
;
BULK INSERT bronze.Customers
From 'C:\Users\Rabeesa Arveen\Downloads\sql-bi project\Customers.csv'
		WITH (
		FirstRow = 2,
		FieldTerminator = ',',
		TABLOCK
		);
